{
  "name": "hossammagdy.dev",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "next src/app/",
    "clean": "rm -rf dist/ src/functions/next/ && mkdir dist",
    "predeploy": "yarn run build",
    "deploy": "NODE_ENV=production firebase deploy",
    "preserve": "yarn run build && yarn run copy-deps && yarn run install-deps",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "build": "yarn run build-public && yarn run build-firebase && yarn run build-app",
    "build-public": "rm -rf dist/public && ln -sf ../src/app/public dist/public",
    "build-firebase": "rm -rf dist/functions && ln -sf ../src/functions dist/functions && cd src/functions && yarn --ignore-engines",
    "build-app": "next build src/app/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hossam-magdy/hossammagdy.dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hossam-magdy/hossammagdy.dev/issues"
  },
  "homepage": "https://hossammagdy.dev",
  "dependencies": {
    "firebase-admin": "^8.9.0",
    "firebase-functions": "^2.1.0",
    "grpc": "^1.22.2",
    "next": "^9.3.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "firebase-functions-test": "^0.1.6",
    "firebase-tools": "^6.1.0",
   "typescript": "^3.8.3",
    "cross-env": "^5.2.0"
  },
  "engines": {
    "node": "10"
  }
}
